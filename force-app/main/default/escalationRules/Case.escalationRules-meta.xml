<?xml version="1.0" encoding="UTF-8"?>
<EscalationRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <escalationRule>
        <fullName>Help</fullName>
        <active>true</active>
        <ruleEntry>
            <businessHours>dvdsds</businessHours>
            <businessHoursSource>Static</businessHoursSource>
            <criteriaItems>
                <field>Case.Reason</field>
                <operation>notEqual</operation>
                <value>Installation</value>
            </criteriaItems>
            <escalationAction>
                <assignedTo>chandra@goud.com</assignedTo>
                <assignedToTemplate>unfiled$public/asdasd</assignedToTemplate>
                <assignedToType>User</assignedToType>
                <minutesToEscalation>330</minutesToEscalation>
                <notifyCaseOwner>false</notifyCaseOwner>
                <notifyTo>kcs4u4@gmail.com</notifyTo>
                <notifyToTemplate>unfiled$public/EmailSample</notifyToTemplate>
            </escalationAction>
            <escalationStartTime>CaseCreation</escalationStartTime>
        </ruleEntry>
        <ruleEntry>
            <businessHoursSource>Case</businessHoursSource>
            <criteriaItems>
                <field>Case.BusinessHoursId</field>
                <operation>equals</operation>
                <value>Default</value>
            </criteriaItems>
            <escalationStartTime>CaseCreation</escalationStartTime>
        </ruleEntry>
    </escalationRule>
    <escalationRule>
        <fullName>sscs</fullName>
        <active>false</active>
        <ruleEntry>
            <businessHours>dvdsds</businessHours>
            <businessHoursSource>Static</businessHoursSource>
            <escalationAction>
                <assignedTo>AccountQueue</assignedTo>
                <assignedToTemplate>unfiled$public/asdasd</assignedToTemplate>
                <assignedToType>Queue</assignedToType>
                <minutesToEscalation>150</minutesToEscalation>
                <notifyCaseOwner>false</notifyCaseOwner>
                <notifyTo>chandra@goud.com</notifyTo>
                <notifyToTemplate>unfiled$public/EmailSample</notifyToTemplate>
            </escalationAction>
            <escalationStartTime>CaseCreation</escalationStartTime>
            <formula>Login_Url__c &lt;&gt; $Setup.FieloEE__MemberPortal__c.FieloEE__Color01__c</formula>
        </ruleEntry>
    </escalationRule>
</EscalationRules>
